clarinetThreeB = \relative c'' { 
\kappaMeterOn 4/4
#(set-accidental-style 'modern)
% A: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "A" \relative c'' {
 	\cutout {s1}  | c8-- r8 r4 r4 c--\mf  |  \cutout {s1} | c8-- r8 r4 c-- c-- |
}
% B: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "B" \relative c'' {
	r8 c4.-- c4-- c-- | \cutout {s1*2} | c4.-- c8-- c4.-- c8-- |
}
% C: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "C" \relative c'' {
	\cutout {s1} | c8-- c4.-- c4-- c--  | \cutout {s1} | g4-- d'-- d-- d--  |
}

% D: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "D" \relative c'' {
	\cutout {s1} | a4. a8-- r2 | \cutout {s1*2} |
}

% E: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "E" \relative c'' {
	\cutout {s1*3} | g8-- g4.-- g4-- g-- |
}

% F: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "F"
	\cutout {s1*4} |

% G: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "G" \relative c''' {
	\cutout {s1} | c4-- c,2. | \cutout {s1} | << c,1 {s2.\mp\> s4\!\p} >> |
}


% H: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "H" \relative c' {
	\cutout {s1} | d8-- e4.-- d8-- e4.-- | 
	\cutout {s1} | a8-- e4.-- e4-- d-- |
}

\pageBreak
%%%%%%% PAGE 2 %%%%%%%%%%%%%%%
\kappaMeterOff
#(set-accidental-style 'neo-modern)
\tempo  \markup \fontsize #2 {"freely, expressive, faster"}
% I: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "I" \relative c' {
	d2(\mp d') \cutoutBox {s1} <>^"repeat 4 or 5 times, speeding up" \continueLine {s1\< s1\!\> <>\!} \kappaXBar
}
% J: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "J"
	\continueLine {s2..\< s8\!} \cutoutBox {s1} <>^"repeat 3 or 4 times, slowing down" \continueLine {s4 s2.\> s2 s2\!} \kappaXBar

% K: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "K"
	\continueLine {s1*2} \cutoutBox {s1}  <>^"repeat 4 or 5 times, speeding up"  \continueLine {s1} \kappaXBar

% L: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "L"
	\continueLine {s1*2} \cutoutBox {s1} <>^"repeat 3 or 4 times, slowing down"  \continueLine {s8 s2..\mp } \kappaXBar

% M: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "M" \relative c'' {
	\tempo  \markup \fontsize #2 {"ritard. gradually until end of variation"}
	<>\mf \continueLine {s1*2} \cutout {s1} g8--\mp c4.-- d4-- e-- \kappaXBar
}

% N: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "N"
	\cutoutBox {s1} <>^"repeat 5 or 6 times, slowing down"  \continueLine {s1*3} \kappaXBar

% O: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "O"
	\continueLine {s1 s2..\> s8\!\p} \cutoutBox {s1} <>^"repeat 4 or 5 times, slowing down" \continueLine {s1} \kappaXBar

% P: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "P"
	<>^"(slow down significantly)" \continueLine {s1*4}  \bar "|."
}