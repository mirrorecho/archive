clarinetThreeA = \relative c'' { 
\kappaMeterOn 4/4
#(set-accidental-style 'modern)
% A: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "A" \relative c'' {
	\cutout {s1}  | g2.\<( a4\!\mf) |  \cutout {s1} | r4 e'8( f a4.)-- c8---> |
}
% B: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "B" \relative c''' {
	r8  c---> c( a) g16( c,8.) f,4 | 
	\cutout {s1*2} | 
	g2.( g'4) |
}
% C: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "C" \relative c'' {
	\cutout {s1} | 
	g8( c d4) e16( g8.) c,8( a) | 
	\cutout {s1} | 
	r4 c4( e,2) |
}
% D: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "D" \relative c' {
	f8( g c d) e16( g8.) a4---> | 
	\cutout {s1*2} | 
	e,4 c16( e8.~ e4) a |
}
% E: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "E" \relative c'' {
	\cutout {s1*2} | c4( d4) e( g) | \cutout {s1}
}
% F: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "F" \relative  c'' {
	\cutout {s1} | 
	g4( e') g,4-- g-- | 
	\cutout {s1} | 
	%fis,2 b8( d4 c8) | (had fis before)
	a2 b8( d4 c8) | 
}
% G: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "G" \relative c'' {
	\cutout {s1} | d2 b | 
	\cutout {s1} | e2\mp\> g,\!\p |
}
% H: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "H" \relative c' {
	\cutout {s1} | d2 c4( e) | \cutout {s1} | e1 |
}
\pageBreak
%%%%%%% PAGE 2 %%%%%%%%%%%%%%%
\kappaMeterOff
#(set-accidental-style 'neo-modern)
\tempo  \markup \fontsize #2 {"freely, expressive, faster"}
% I: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "I" \relative c' {
	\tuplet 5/4 {fis4_(\mp g b g fis)} 
	\cutoutBox {s1} 
	<>^"repeat 4 or 5 times, speeding up" \continueLine {s1*2}
\kappaXBar }
% J: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "J" \relative c'' {
	\continueLine { s2 \hideNotes g2\< \unHideNotes s2.. s8\!} \cutout {s1*2} \kappaXBar
}
% K: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "K" \relative c'' {
	a16[( c8.]) c8( b4.) a16[( c8.])
	\tuplet 3/2 {c8[( b g]} \tuplet 3/2 {fis) d'4(} c8 b4.)
	e16[( g8.)] e16[( g8.)]  \tuplet 3/2 {g4( d g,)}
	\cutout {s1} \kappaXBar
}
% L: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "L"
	\cutoutBox {s1*2} <>^"repeat 3 or 4 times, skipping one or two notes each time"
	\continueLine {s1*2}  \kappaXBar

% M: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "M"
\tempo  \markup \fontsize #2 {"ritard. gradually until end of variation"}
\continueLine {s1*3} \cutout {s1} \kappaXBar

% N: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "N" \relative c'' {
	\tuplet 5/4 {g4( c g) a( e')} 
	\cutout {s1*2} 
	r2 g,\mf \kappaXBar
}
% O: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "O" \relative c'' {
	\tuplet 3/2 {g2(\> a) e(}
		\tuplet 5/4 {c4) e( g a) e--\!\p}
 	\cutoutBox {s1} <>^"repeat 3 or 4 times, slowing down" 
 	\continueLine {s1}
\kappaXBar }

% P: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "P"
\continueLine {s1*3} e1\fermata  \bar "|."

}

