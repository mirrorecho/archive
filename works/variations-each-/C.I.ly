clarinetI = \relative c' { #(set-accidental-style 'modern)
\kappaMeterOn 4/4
\tempo  \markup \fontsize #2 { \note #"4" #1 = 104 ca}

% A: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "A" \relative c' {
	\once \override Hairpin #'circled-tip = ##t
	e2(\< f)\!\pp | 
	\unique "C.I.two.a" { \relative c'' {e2\< a16-.->\!\sfz a-.-> <<e4.\> { s4 s8\!} >> } } | 
	e1 | \unique "C.I.two.a" { \relative c''' { r8 a-. a4\mp\< e'16-.->\!\sfz e-.-> <<a,4.\> { s4 s8\!} >>  }} |
}
% B: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "B"
	\unique "C.I.one.b" {\relative c''' {
		a8-- e-- c( b) a-- e-- f( a)}} | 
	\relative c' { e2( c) | r2 r4 f  | }
	\unique "C.I.one.b" {\relative c''' {
		a8-- e( f) c-- a8-- e( f) c-- }} |
%{
a2.( f4) | \tuplet 3/2 {e4( f e)} \tuplet 3/2 {e4( f) c'} | 
	b2 ~ b8 r e( f)  | a4( f) a4( f) |
%}

% C: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "C" \relative c' {
	e2 r4 f | 
	a4( f) a( f) | 
	e2( c) | 
	b1 |
}

% D: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "D" \relative c' {
	r4 e2( f4) | 
	\unique "C.I.three.a" { \relative c''' {g8( c g e) d( c a g) } } | 
	f1 | e |
}
% E: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "E" \relative c' {
	e2 e16( g8.~ g4)) | 
	g4( <>) << g'2. {s2\< s8.\!\> s16\! } >>  |
	g,4( <>) << g'2. {s2\< s8.\!\> s16\! } >>  | 
	r2 g,4-- g-- |
}
% F: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "F" \relative c' {
	% USE A CROSS SLUR HERE (for last 2 bars... because I can)
	fis1 | 
	\unique "C.I.three.b" \relative c'' { g8-- c4.-- d4-- e-- } | 
	r4 g(\< g') g,--\! | 
	c8( g'4. ~ g2) |
}
% G: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "G" \relative c'' {
	e2.\> r4\! | \unique "C.I.two.a" { aes,8( g4.) ees'4( ees,)} | g2 e | 
	}
	\unique "C.I.two.a" { \relative c''' {
		g16-.->\ff g-.-> d-.-> d-.-> r8 g,16-.-> g-.-> r8 d4.--\ppp
	}
	} 
% H: ---------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "H" \relative c' {
	b1 | 
	\unique "C.I.two.a" {aes8(\p g4.) bes2 } | 
	g'8(\> fis4.) g8( fis4.)\!  | 
	\unique "C.I.two.a" { f2 ~ f4. e16-. e-. } |
}
\pageBreak
%%%%%%% PAGE 2 %%%%%%%%%%%%%%%
\kappaMeterOn 4/4
% I: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "I" \relative c' {
 	c1^\markup \column {"Instead of playing the following music," 
 						"describe it in detail. Speak loudly and" 
 						"clearly, until someone interupts."} |  
	r4 d2( e4) | \unique "C.I.two.a" {e'16-.->\f e-.-> r8 r4 r4 fis,8---> g'16-.-> g-.-> } |
	r4 e16-.-> e-.-> r8 r4 fis,8---> g'16-.-> g-.-> |
}
% J: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "J" \relative c'' {
	\unique "C.I.two.a" {r4 e16-.-> e-.-> r8 r4 d16-.-> d-.-> d8--\mp} | 
	cis,4\mp cis8( d) r2 | 
	\unique "C.I.three.a" { 
		\tuplet 3/2 {g8[( c g])} a16[( c8.]) \tuplet 5/4 {e8(\< g a c4)\!\mf} \bar ""
		c,8[( c']) a[( c])\> \tuplet 3/2 {g8[( e d]} \tuplet 3/2 {c[ a g])\!}
	} |
}
% K: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "K" \relative c'' {
	\unique "C.I.two.a" {r4 e16-.->\f e-.-> r8 fis,8---> g'16-.-> g-.-> r4 |  
	aes,,8( bes) ees( aes ~ aes4) ees-- | }
	b'4-- e,2 r4 | 
	d8( e) g( a) c( a g4) |
}
% L: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "L"
\unique "C.I.three.a" \relative c'' {
	\tuplet 3/2 {g4( a c~ } c16[) e]( \tuplet 3/2 {g8 a4~} a8 \bar "" 
		\once \override TupletNumber.text = #tuplet-number::calc-fraction-text
		\tuplet 4/3 {g4) e8[( d]}
		c8[ a]) g'8~( \tuplet 3/2 {g8[ d c])}

	} | 
	\relative c''' { r4 a2\> f,4\!\pp | e2. r4 } |

% M: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "M" \relative c''' {
	c1^\markup \column {"Scream and shout!" } | \unique "C.I.one.b" {a8-- e-- c( b) a-- e-- f!( a)} | 
	e1 | r2 f |
}

% N: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "N" \relative c'' {
	a2.( g4) | 
	\unique "C.I.three.a" \relative c''' { 
		g8( e[ d]) \tuplet 5/4 {c8( a g a) d(} g\noBeam)
		\tuplet 3/2 {c8([\< g c)]} \tuplet 3/2 {a8[( c) a](} \tuplet 5/4 {c) d( e g4)\!\f\fermata }
		} \bar ""
	a4( f) a( f)  |
}
% O: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "O" \relative c''' {
	\unique "C.I.one.b" {g8-- d( fis) b,-- r4 c} | 
	\unique "C.I.two.a" { aes4 bes16-.->\ff bes16-.-> r8 aes4\pp bes16-.->\ff bes16-.-> bes8--\pp } | 
	 e,2( c) | b1 |
}

% P: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "P"
\unique "C.I.one.a" { r4 e2( f4) |  f4( a) a( f) | f1 e1 }  \bar "|."

}