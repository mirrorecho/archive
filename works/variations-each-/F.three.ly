fluteThreeA = \relative c'' { 

% A: ------------------------------------------------------------------------------------------------------------------------------------------------------
\kappaMeterOff
#(set-accidental-style 'neo-modern)

\tempo  \markup \fontsize #2 {"freely, " \note #"4" #1 "= 60 ca"}
\newLine "A" \relative c'''' {
	\once \override DynamicLineSpanner.staff-padding = #4
	a2\mp\>  \tuplet 3/2 {e4( a,4) c,8( b8)\!\p }  
	\cutoutBox {s1} <>^" repeat 7 or 8 times, change the rhythm slightly each time"
	\continueLine {s1*2} \kappaXBar
}

% B: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "B"
%\cutoutBox{s1}  
\continueLine {s1*4} \kappaXBar

% C: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "C" \relative c''' {
	\continueLine {s1*2}  \cutout {s1}  \tuplet 5/4 {g4( b2) c,4( b)} \kappaXBar
}
% D: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "D"
\cutoutBox {s1}  <>^" repeat 4 or 5 times, change the rhythm slightly each time"
\continueLine {s1*3} \kappaXBar
%\startCutout s1 \stopCutout | d16 d d d d d d d d d d d d4 | c1

% E: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "E"
\cutoutBox {s1}  <>^" repeat 3 or 4 times, change the rhythm slightly each time"
	<< \continueLine {s1*3}  {s1 s1\p s1 } >>
\kappaXBar

% F: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "F" \relative c'' {
	\tuplet 3/2 {a2) e''1}
	\cutoutBox {s1} <>^" repeat 4 or 5 times, change the rhythm slightly each time"
	\continueLine {s1*2} \kappaXBar
}
% G: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "G"
\continueLine {s1*3} \cutoutBox {s1}   <>^" repeat 3 or 4 times, change the rhythm slightly each time"
\kappaXBar

% H: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "H"
\continueLine {s1*4} \kappaXBar

\pageBreak
%%%%%%% PAGE 2 %%%%%%%%%%%%%%%
\kappaMeterOn 4/4
% I: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "I" \relative c''' {
	#(set-accidental-style 'modern)
	\cutout {s1} | g2( b,) | \cutout {s1} | r4 g''2( b,,4) |
}
% J: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "J" \relative c'''' {
	\cutout {s1} | r4 a2( e,4) | \cutout {s1*2}
}
% K: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "K" \relative c''' {
	\cutout {s1} | a4 e'8( a8~ a4) a, | 
	\cutout {s1} | a2( c,2) |
}
% L: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "L" \relative c''' {
	a2 c2 | \cutout {s1} | a1 | \cutout {s1} | 
}
% M: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "M" \relative c'' {
	\cutout {s1} | c8( b4.~ b2) | \cutout {s1} | r4 g''2. |
}
% N: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "N" \relative c'' {
	r4 a2. | \cutout {s1*2} | a''1 |
}
% O: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "O" \relative c''' {
	\cutout {s1} | c8( a'4.) c,,2 | \cutout {s1} | R1 |
}
% P: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "P" \relative c'''' {
	% ?? cutout middle to bars?
	\cutout {s1} | a2. r4 | \cutout {s1} | g1  \bar "|."
}
}

