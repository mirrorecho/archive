fluteThreeB = \relative c'' { 
\kappaMeterOff
#(set-accidental-style 'neo-modern)

\tempo  \markup \fontsize #2 {"freely, " \note #"4" #1 "= 60 ca"}
% A: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "A" \relative c' {
	d2(\p d') \cutoutBox {s1} <>^" repeat 5 or 6 times, speeding up"
	\continueLine {s1*2} \kappaXBar
}

% B: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "B"
	\continueLine {s1*3} \cutoutBox {s1} \kappaXBar

% C: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "C"
	<>^" repeat 2 or 3 times, speeding up"
	\continueLine {s1*2} \cutoutBox {s1}  <>^" repeat 2 or 3 times, speeding up"
	\continueLine {s1} \kappaXBar

% D: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "D"
	\continueLine {s1} \cutoutBox {s1} <>^" repeat 3 or 4 times, speeding up"
	\continueLine {s1*2} \kappaXBar
%\startCutout s1 \stopCutout | d16 d d d d d d d d d d d d4 | c1

% E: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "E"
	\continueLine {s1} \cutoutBox {s1} <>^" repeat 3 or 4 times, speeding up"
	\continueLine {s1*2} \kappaXBar

% F: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "F"
	\continueLine {s1} \cutoutBox {s1} <>^" repeat 3 or 4 times, speeding up"
	\continueLine {s1*2} \kappaXBar

% G: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "G"
	\continueLine {s1} \cutoutBox {s1} <>^"repeat 3 or 4 times, slowing down"  \continueLine {s1*2} \kappaXBar

% H: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "H"
	\continueLine {s1*2} \cutout {s1*2} \kappaXBar

\pageBreak
%%%%%%% PAGE 2 %%%%%%%%%%%%%%%
\kappaMeterOn 4/4
#(set-accidental-style 'modern)

% I: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "I" \relative c'' {
	#(set-accidental-style 'modern)
	\cutout {s1} | r4 d2( d'4) | \cutout {s1} | d8(\< g,) d'8( g,) d'8(\!\> g,) d'8( g,)\! |
}

% J: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "J" \relative c'' {
	\cutout {s1} | r4 d4-- g( d') \cutout {s1*2} |
}

% K: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "K" \relative c''' {
	\cutout {s1} | d8(\mp g,) d'8( g,) d'8( g,) d'4 | \cutout {s1*2} |
}

% L: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "L" \relative d''{
	%perhaps this would work better if it matches F.one.b cutouts instead? (1st and 3rd)
	r4 d\< d'8--\mf r8 d-- d--  | \cutout {s1} | r4 d,2\> d'4\mp | \cutout {s1} | 
}
% M: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "M" \relative c'' {
	\cutout {s1} | d2.( d'4 )| \cutout {s1} | r4 g,2. |
}

% N: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLine "N" \relative c'' {
	d2.( d'4) | \cutout {s1*2} | d1 |
}
% O: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "O" \relative c''' {
	\cutout {s1} | d1 | \cutout {s1} | d,1 |
}

% P: ------------------------------------------------------------------------------------------------------------------------------------------------------
\newLineCutout "P" \relative c'' {
	% ?? cutout middle two bars?
	\cutout {s1} | d4( d') d( g,) | \cutout {s1} | d1  \bar "|."
}
}